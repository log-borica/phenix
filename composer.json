{
    "name": "phenix/phenix",
    "description": "Elasticsearch ORM for Laravel/Lumen",
    "keywords": [
        "phenix",
        "aws",
        "elasticsearch",
        "laravel",
        "lumen",
        "php"
    ],
    "homepage": "https://github.com/the-phenix/phenix",
    "type": "library",
    "require": {
        "php": ">=7.2",
        "aws/aws-sdk-php": "~3.0",
        "elasticsearch/elasticsearch": "~7.0",
        "nesbot/carbon": "^2.29.1",
        "guzzlehttp/guzzle": "~6.5",
        "logcomex/php-utils": "^0.8.1"
    },
    "require-dev": {
        "illuminate/contracts": "^6.0",
        "illuminate/support": "^6.0",
        "illuminate/http": "^6.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Crystopher Glodzienski Cardoso",
            "email": "crystopher.glodzienski@gmail.com",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Phenix\\Core\\": "src"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "0.x-dev"
        },
        "laravel": {
            "providers": [
                "Phenix\\Core\\ElasticSearchServiceProvider"
            ],
            "aliases": {
                "ElasticSearch": "Phenix\\Core\\ElasticSearchFacade"
            }
        }
    }
}
